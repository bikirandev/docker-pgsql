# n8n Container Configuration
N8N_CONTAINER_NAME=n8n-container

# n8n Server Configuration
N8N_HOST=localhost
N8N_PORT=5678
N8N_PROTOCOL=http

# Node Environment
NODE_ENV=production

# Webhook URL (for external access)
# Example: https://your-domain.com/
WEBHOOK_URL=http://localhost:5678/

# Timezone Configuration
GENERIC_TIMEZONE=UTC

# Admin User Configuration
N8N_DEFAULT_USER_EMAIL=admin@example.com
N8N_DEFAULT_USER_PASSWORD=changeme123

# Volume Name
N8N_VOLUME_NAME=n8n_data_volume